rating = input("Rate from 1-10: ")
rating = int(rating)
penup()
pensize(10)
if rating <= 4:
    goto(-50,50)
    color("red")
    pendown()
    goto(50,-50)
    penup()
    goto(-50,-50)
    pendown()
    goto(50,50)
elif rating >= 8:
    goto(-50,0)
    color("green")
    pendown()
    goto(0,-50)
    goto(50,50)
else:
    goto(-50,0)
    color("yellow")
    pendown()
    goto(50,0)
